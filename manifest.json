{
  "name": "Pumba Search",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Добавление поиска на «Пумбе» для некоторых киносайтов",
  "page_action": {},
  "content_scripts": [
    {
      "matches": ["http://www.kinopoisk.ru/film/*"],
      "css": ["css/base.css"],
      "js": ["js/base.js", "js/kinopoisk.js"],
      "run_at": "document_end"
    }
  ]
}
